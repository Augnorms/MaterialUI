{
  "compilerOptions": {
    "jsx": "react",
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "composite": true,
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "allowSyntheticDefaultImports": true
  },
  "include": ["src/**/*", "types/**/*", "custom_typings/**/*"]
}
